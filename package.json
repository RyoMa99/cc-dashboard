{
	"name": "cc-dashboard",
	"version": "0.1.0",
	"private": true,
	"packageManager": "pnpm@10.29.3",
	"type": "module",
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit",
		"lint": "biome check src/",
		"db:migrate:local": "wrangler d1 migrations apply cc-dashboard-db --local",
		"db:migrate:remote": "wrangler d1 migrations apply cc-dashboard-db --remote",
		"db:seed": "tsx scripts/seed.ts && wrangler d1 execute cc-dashboard-db --local --file=scripts/.seed.sql"
	},
	"dependencies": {
		"hono": "^4.7.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": ["@biomejs/biome", "esbuild", "workerd"]
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.2",
		"@cloudflare/vitest-pool-workers": "^0.12.13",
		"@cloudflare/workers-types": "^4.20260217.0",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^3.0.0",
		"wrangler": "^4.66.0"
	}
}
